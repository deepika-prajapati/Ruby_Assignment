<h1>Employee Details</h1>
<%= link_to "Add New Employee", new_employee_info_path %>
<br />
<br />
<table border="1">
  <tr>
    <th>First Name </th>              
    <th>Last Name </th>                      
    <th>Employee Id </th>
    <th>Gender </th>   
    <th>Highest Qualification </th>  
    <th>Click here to change or delete the employee details </th>   
  </tr>
 <% @employeeInfo.each do |employeeInfo| %>
  <tr>
    <td><%=employeeInfo.first_Name%></td>         
    <td><%=employeeInfo.last_Name %></td>   
    <td><%=employeeInfo.empId %></td>  
    <td><%=employeeInfo.gender %></td>  
    <td><%=employeeInfo.highest_Degree %></td>   
    <td> <a href="/employee_infos/<%= employeeInfo.id %>/show">Edit here</a>
     </td>
  </tr>
  <% end %>
 </table>

